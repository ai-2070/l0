    "./adapters/helpers": {
      "types": "./dist/adapters/helpers.d.ts",
      "import": "./dist/adapters/helpers.js",
      "default": "./dist/adapters/helpers.js"
    },
    "./guardrails": {
      "types": "./dist/guardrails.d.ts",
      "import": "./dist/guardrails.js",
      "default": "./dist/guardrails.js"
    },
    "./drift": {
      "types": "./dist/drift.d.ts",
      "import": "./dist/drift.js",
      "default": "./dist/drift.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "directories": {
    "lib": "dist",
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "prettier": "prettier --write .",
    "prepublishOnly": "npm run build"
  },
